<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.store.StoreController">
   <children>
      <Label id="nameLabel" fx:id="nameLabel" styleClass="subTitle" />
      <Label id="detailsLabel" fx:id="detailsLabel" />
      <ScrollPane prefHeight="278.0" prefWidth="504.0">
         <content>
            <FlowPane fx:id="storeProducts" prefHeight="273.0" prefWidth="557.0" />
         </content>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </ScrollPane>
   </children>
   <styleClass>
      <String fx:value="card" />
      <String fx:value="store" />
   </styleClass>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
